{
    "env": {
        "development": {
            "text/typescript": {
                "module": "commonjs",
                "target": "es2015",
                "jsx": "react",
                "strict": true,
                "importHelpers": true,
                "noEmitHelpers": true,
                "noImplicitAny": true,
                "experimentalDecorators": true,
                "preserveConstEnums": true,
                "noImplicitReturns": true,
                "noUnusedLocals": true,
                "noImplicitThis": true,
                "noUnusedParameters": true,
                "inlineSourceMap": true,
                "inlineSources": true,
                "removeComments": false
            }
        },
        "production": {
            "text/typescript": {
                "module": "commonjs",
                "target": "es2015",
                "jsx": "react",
                "strict": true,
                "importHelpers": true,
                "noEmitHelpers": true,
                "noImplicitAny": true,
                "experimentalDecorators": true,
                "preserveConstEnums": true,
                "noImplicitReturns": true,
                "noUnusedLocals": true,
                "noImplicitThis": true,
                "noUnusedParameters": true,
                "inlineSourceMap": false,
                "inlineSources": false,
                "removeComments": true
            }
        }
    }
}
