{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2015",
        "jsx": "react",
        "strict": true,
        "importHelpers": true,
        "noEmitHelpers": true,
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "declaration": false,
        "preserveConstEnums": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
        "noLib": false,
        "outDir": "../out",
        "rootDir": "./"
    },
    "files": [
        "main/index.ts",
        "renderer/index.ts",
        "renderer/repository-list-view.tsx"
    ]
}
