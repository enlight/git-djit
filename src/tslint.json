{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "object-literal-sort-keys": false,
        "max-classes-per-file": false,
        "member-access": [true, "no-public"],
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"],
        "no-namespace": false
    }
}
