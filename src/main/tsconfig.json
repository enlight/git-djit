{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2015",
        "strict": true,
        "importHelpers": true,
        "noEmitHelpers": true,
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "preserveConstEnums": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "declaration": true,
        "noLib": false,
        "outDir": "../../out",
        "rootDir": "../"
    },
    "files": [
        "index.ts"
    ]
}
